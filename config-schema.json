{
  "schemas": {
    "v3c": {
      "connection": {
        "database": "PgVectorConnectionProvider",
        "parameters": {
          "password": "postgres",
          "username": "postgres"
        }
      },
      "fields": {
        "clipmap": {
          "factory": "CLIP",
          "parameters": {
            "host": "http://127.0.0.1:8888"
          }
        },
        "file": {
          "factory": "FileSourceMetadata"
        },
        "time": {
          "factory": "TemporalMetadata"
        },
        "video": {
          "factory": "VideoSourceMetadata"
        }
      },
      "resolvers": {
        "disk": {
          "factory": "DiskResolver",
          "parameters": {
            "location": "./thumbnails",
            "mimeType": "JPG"
          }
        }
      },
      "exporters": {
        "thumbnail": {
          "factory": "ThumbnailExporter",
          "resolverName": "disk",
          "parameters": {
            "maxSideResolution": "400",
            "mimeType": "JPG"
          }
        }
      }
    },
